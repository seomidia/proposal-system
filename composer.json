{
    "name": "seomidia/meu-plugin",
    "type": "project",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "laravel/breeze": "^2.0",
        "laravel/sanctum": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
